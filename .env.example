PORT=3000
ENV=development
# Gemini API Key
API_KEY=MY_API_KEY

# Upstash REST for server.js (used when running the Express server or similar)
UPSTASH_REDIS_REST_URL=https://rest.upstash.io/v2/<INSTANCE_ID>
UPSTASH_REDIS_REST_TOKEN=<INSTANCE_TOKEN>
# Cache TTL in seconds (default: 1 week). Used both locally and in serverless.
CACHE_TTL_SECONDS=604800
